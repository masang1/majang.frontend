{
  "expo": {
    "name": "마법의 장터",
    "slug": "마법의 장터",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "majang",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundler": "metro",
      "supportsTablet": true
    },
    "android": {
      "bundler": "metro",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}